@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix book: <http://www.w3.org/2002/01/bookmark#> .


<#BookmarkResource> a rdfs:Resource
  ; rdfs:isDefinedBy <./>
  ; rdfs:label "BookmarkResource"
  ; rdfs:comment "A resource containing a single bookmark"
  .

<#hasBookmark> a rdf:Property
  ; rdfs:label "hasBookmark"
  ; rdfs:comment "The subject resource has the single object bookmark"
  ; rdfs:domain <#BookmarkResource>
  ; rdfs:range book:Bookmark
  .
