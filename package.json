{
  "name": "mark",
  "version": "0.1.0",
  "description": "A small bookmarks app for Solid - https://solid.mit.edu/",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "postinstall": "npm run build",
    "start": "node prod-server.js",
    "dev": "node dev-server.js",
    "lint": "standard --verbose 'src/**/*.js' 'test/**/*.js'",
    "mocha": "mocha --growl --require babel-register test/**/*.js",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc --reporter=html npm run mocha",
    "test:dev": "cross-env NODE_ENV=test npm run mocha -- --watch --inspect",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-f/mark.git"
  },
  "keywords": [
    "solid",
    "bookmarks",
    "web",
    "decentralization",
    "linked",
    "data"
  ],
  "author": "Daniel Friedman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dan-f/mark/issues"
  },
  "homepage": "https://github.com/dan-f/mark#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "compression": "^1.6.2",
    "connect": "^3.6.0",
    "connect-timeout": "^1.8.0",
    "css-loader": "^0.28.4",
    "immutable": "^4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-loading-overlay": "^0.2.4",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "serve-static": "^1.12.1",
    "solid-auth-client": "^0.3.4",
    "solid-auth-tls": "^0.1.2",
    "solid-web-client": "^0.3.0",
    "style-loader": "^0.18.2",
    "url-join": "^1.1.0",
    "uuid": "^2.0.3",
    "valid-url": "^1.0.9",
    "webpack": "^2.2.1"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**.js"
    ],
    "exclude": [
      "test/**.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "localStorage"
    ]
  }
}
