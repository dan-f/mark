{
  "name": "mark",
  "version": "0.1.0",
  "description": "A small bookmarks app for Solid - https://solid.mit.edu/",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "start": "node server.js",
    "lint": "standard --verbose src/**/*.js test/**/*.js",
    "mocha": "mocha --growl --require babel-register test/**/*.js",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test nyc --reporter=html npm run mocha",
    "test:dev": "npm run mocha -- --watch --debug",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-f/mark.git"
  },
  "keywords": [
    "solid",
    "bookmarks",
    "web",
    "decentralization",
    "linked",
    "data"
  ],
  "author": "Daniel Friedman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dan-f/mark/issues"
  },
  "homepage": "https://github.com/dan-f/mark#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "react-hot-loader": "^3.0.0-beta.5",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.16.4",
    "modelld": "^0.11.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-solid-auth": "^0.2.0",
    "redux-thunk": "^2.1.0",
    "solid-client": "^0.23.7",
    "url-join": "^1.1.0",
    "uuid": "^2.0.3",
    "valid-url": "^1.0.9"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**.js"
    ],
    "exclude": [
      "test/**.js"
    ]
  }
}
